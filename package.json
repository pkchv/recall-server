{
  "name": "recall-server",
  "version": "1.0.0",
  "description": "Spaced repetition software",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "NODE_ENV=dev nodemon src/index.ts | pino-pretty",
    "dev:debug": "NODE_ENV=dev nodemon --exec 'node -r ts-node/register --inspect-brk' server/index.ts | pino-pretty",
    "test:all": "NODE_ENV=test mocha test/unit/**/*.ts test/integration/**/*.ts --exit",
    "test:unit": "NODE_ENV=test mocha test/unit/**/*.ts --exit",
    "test:integration": "NODE_ENV=test mocha test/integration/**/*.ts --exit",
    "docker:up": "docker-compose -f docker-compose.yaml up",
    "docker:down": "docker-compose -f docker-compose.yaml down"
  },
  "dependencies": {
    "ajv": "^6.10.0",
    "async": "^2.6.2",
    "body-parser": "^1.18.3",
    "boom": "^7.3.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "helmet": "^3.15.1",
    "http-status": "^1.3.1",
    "jsonwebtoken": "^8.5.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "password-validator": "^4.1.1",
    "pg": "^7.8.1",
    "pino": "^5.8.1",
    "reflect-metadata": "^0.1.13",
    "route-decorators": "^0.2.2",
    "safe-compare": "^1.1.3",
    "sqlite3": "^4.0.6",
    "swagger-express-middleware": "^1.1.1",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.16",
    "typeorm-typedi-extensions": "^0.2.3"
  },
  "devDependencies": {
    "@types/boom": "^7.2.1",
    "@types/chai": "^4.1.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/lodash": "^4.14.121",
    "@types/mocha": "^5.2.5",
    "@types/moment": "^2.13.0",
    "@types/node": "^11.9.5",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/pino": "^5.20.0",
    "@types/sinon": "^7.0.9",
    "@types/sinon-chai": "^3.2.2",
    "@types/sinon-express-mock": "^1.3.7",
    "@types/supertest": "^2.0.7",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "faker": "^4.1.0",
    "mocha": "^5.0.5",
    "nodemon": "^1.18.9",
    "pino-pretty": "^2.5.0",
    "shelljs": "^0.8.3",
    "sinon": "^7.2.7",
    "sinon-chai": "^3.3.0",
    "sinon-express-mock": "^2.1.0",
    "supertest": "^3.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.2.2",
    "uuid": "^3.3.2"
